{
  "name": "fienta-code-manager",
  "version": "1.0.0",
  "description": "Manage Fienta discount codes via Playwright automation (create, rename, update)",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/cli.ts",
    "dev:enhanced": "tsx src/cli-enhanced.ts",
    "start": "node dist/cli.js",
    "build": "tsc -p tsconfig.json",
    "playwright:install": "playwright install",
    "lint": "echo 'no linter configured'",
    "xlsx:to:csv": "tsx src/tools/xlsx_to_csv.ts",
    "scrape:usage": "tsx src/cli-enhanced.ts scrape-usage",
    "scrape:details": "tsx src/cli-enhanced.ts scrape-code-details",
    "export:buyers": "tsx src/cli-enhanced.ts export-buyers"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "pino": "^9.9.0",
    "playwright": "^1.55.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.1.12",
    "@types/node": "^24.3.0",
    "pino-pretty": "^13.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "zx": "^8.8.1"
  }
}
