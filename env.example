# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Webhook Authentication
MAKE_TOKEN=your-make-webhook-token-here

# Fienta Automation Credentials
FIENTA_EMAIL=your-fienta-email@example.com
FIENTA_PASSWORD=your-fienta-password
FIENTA_BASE_URL=https://fienta.com

# Gmail/Email Configuration (Optional)
GMAIL_CREDENTIALS_PATH=archive/email_outreach/client_secret.json
GMAIL_TOKEN_PATH=archive/email_outreach/token.json

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
CORS_ORIGINS=["http://localhost:3000", "https://your-frontend-domain.com"]

# Job Execution Settings
JOB_TIMEOUT_SECONDS=3600
MAX_CONCURRENT_JOBS=3

# Deployment (for production)
PORT=8000
